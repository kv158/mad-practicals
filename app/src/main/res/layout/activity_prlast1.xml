<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".prlast1">

    <!--        app:layout_constraintBottom_toBottomOf="parent"--><![CDATA[
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.36"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/java" />



    ]]>

    <TextView
        android:id="@+id/tvd"
        android:layout_width="368dp"
        android:layout_height="191dp"
        android:layout_marginBottom="72dp"
        android:shadowColor="@color/kab"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/py"
        android:layout_width="252dp"
        android:layout_height="308dp"
        android:layout_marginBottom="96dp"
        app:layout_constraintBottom_toTopOf="@+id/tvd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/py" />

    <ImageView
        android:id="@+id/js"
        android:layout_width="252dp"
        android:layout_height="308dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/tvd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.483"
        app:srcCompat="@drawable/js" />

    <ImageView
        android:id="@+id/java"
        android:layout_width="252dp"
        android:layout_height="308dp"
        android:layout_marginBottom="136dp"
        app:layout_constraintBottom_toTopOf="@+id/tvd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/java" />

    <ImageView
        android:id="@+id/c"
        android:layout_width="252dp"
        android:layout_height="308dp"
        android:layout_marginBottom="96dp"
        app:layout_constraintBottom_toTopOf="@+id/tvd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/c" />

    <ImageView
        android:id="@+id/cpp"
        android:layout_width="252dp"
        android:layout_height="308dp"
        android:layout_marginBottom="104dp"
        app:layout_constraintBottom_toTopOf="@+id/tvd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.624"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/cpp" />
</androidx.constraintlayout.widget.ConstraintLayout>