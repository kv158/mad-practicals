<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Spinnerp">

    <Spinner
        android:id="@+id/spr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="1dp"
        android:entries="@array/kvlist"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.089" />

    <ImageView
        android:id="@+id/k1"
        android:layout_width="260dp"
        android:layout_height="437dp"
        android:layout_marginTop="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spr"

        app:srcCompat="@drawable/k1"
        tools:layout_editor_absoluteX="75dp" />

    <ImageView
        android:id="@+id/k2"
        android:layout_width="260dp"
        android:layout_height="437dp"
        app:srcCompat="@drawable/k2"
        tools:layout_editor_absoluteX="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spr"
        tools:layout_editor_absoluteY="202dp" />

    <ImageView
        android:id="@+id/k3"
        android:layout_width="260dp"
        android:layout_height="437dp"
        android:layout_marginBottom="92dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spr"
        app:srcCompat="@drawable/k3" />

    <ImageView
        android:id="@+id/k4"
        android:layout_width="260dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spr"
        android:layout_height="437dp"
        app:srcCompat="@drawable/k4"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="202dp" />

    <ImageView
        android:id="@+id/k5"
        android:layout_width="260dp"
        android:layout_height="437dp"
        android:layout_marginTop="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spr"

        app:srcCompat="@drawable/k5"
        tools:layout_editor_absoluteX="85dp" />
</androidx.constraintlayout.widget.ConstraintLayout>